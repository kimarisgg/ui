{
  "name": "@kimarisgg/ui",
  "version": "0.2.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "deps:check": "npx npm-check-updates -i",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "play": "nuxi dev playground --port 3001",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@floating-ui/dom": "^1.6.5",
    "@floating-ui/vue": "^1.0.6",
    "@nuxt/kit": "^3.11.2",
    "@unocss/nuxt": "^0.60.2",
    "nuxt-lucide-icons": "^1.0.4",
    "radix-vue": "^1.7.4"
  },
  "peerDependencies": {
    "lucide-vue-next": "^0.376.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nuxt/module-builder": "^0.6.0",
    "husky": "^9.0.11",
    "nuxt": "^3.11.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "resolutions": {
    "@nuxt/kit": "^3.11.2",
    "@nuxt/schema": "3.11.2",
    "vue": "3.4.21"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-case": [
        0
      ],
      "subject-case": [
        2,
        "always",
        "sentence-case"
      ],
      "header-max-length": [
        0
      ]
    }
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "printWidth": 120
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write"
    ]
  }
}
